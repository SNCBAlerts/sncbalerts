{
    "name": "drupol/sncbdelay",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "ext-pdo": "*",
        "drupol/sncbdelay_telegram": "*",
        "drupol/sncbdelay_twitter": "*",
        "drupol/sncbdelay_web": "*",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/cache": "^4",
        "symfony/console": "^4",
        "symfony/contracts": "^1",
        "symfony/expression-language": "^4",
        "symfony/flex": "^1",
        "symfony/framework-bundle": "^4",
        "symfony/http-client": "^4.3@dev",
        "symfony/lock": "^4",
        "symfony/monolog-bundle": "^3",
        "symfony/orm-pack": "^1",
        "symfony/process": "^4",
        "symfony/twig-bundle": "^4",
        "symfony/yaml": "^4"
    },
    "require-dev": {
        "symfony/web-server-bundle": "^4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "symfony/dotenv": "^4",
        "symfony/var-dumper": "^4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "drupol\\sncbdelay\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0JSWYHW11GXGCHPSEJT6KC1",
            "allow-contrib": false
        }
    }
}
